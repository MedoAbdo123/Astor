{
  "version": 2,
  "env": {
      "MONGO_URL": "mongodb://127.0.0.1/Astor",
      "SECRET_KEY": "your-256-bit-secret",
      "PORT": "3000"
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PATCH",
              "DELETE"
          ]
      }
  ]
}
